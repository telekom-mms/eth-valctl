{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "commitMessageAction": "Update",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "groupName": "npm non-major",
      "matchDatasources": [
        "npm"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ]
    }
  ]
}
